{
    "expo": {
        "name": "Slate Plus",
        "slug": "blank-slate",
        "icon": "./assets/icon.png",
        "version": "1.0.0",
        "orientation": "default",
        "scheme": "slate-plus",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.mblatz.slateplus",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#E6F4FE",
                "foregroundImage": "./assets/images/android-icon-foreground.png",
                "backgroundImage": "./assets/images/android-icon-background.png",
                "monochromeImage": "./assets/images/android-icon-monochrome.png"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "package": "com.mblatz.slateplus"
        },
        "web": {
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff",
                    "dark": {
                        "backgroundColor": "#000000"
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "a4e8f7d7-64c0-4fc3-8148-2749e66d7de9"
            }
        },
        "owner": "mblatz",
        "updates": {
            "url": "https://u.expo.dev/a4e8f7d7-64c0-4fc3-8148-2749e66d7de9"
        },
        "runtimeVersion": {
            "policy": "appVersion"
        }
    }
}
